#include<iostream>
using namespace std;
class bouncycheck
{
	public:
	void check()
	{
		int  a[300];
		int ctr1=0,ctr2=0,ctr3=0;
		for(int i=0;i<4;i++)
		{
		cin>>a[i];
		}
		
		for(int i=0;i<4;i++)
		{
			if(a[i]!=7)
			{
			ctr1++;
			}
		}/*cout<<ctr1<<"\n";*/
		for(int i=0;i<4;i++)
		{
			if(a[i+1]>a[i])
			{ctr2++;}
		}	/*cout<<ctr2+1<<"\n";*/
			
			
			
			 for(int i=0;i<4;i++)
                	{
                        if(a[i+1]<a[i])
                        {ctr3++;}
                       	}
			if(ctr3==ctr1)
			{
			cout<<"decreasing"<<"\n";
			}
			else if(ctr2==ctr1)
                        {cout<<"increasing"<<"\n";}


			else 
			{
			cout<<"bouncy"<<"\n";
			}	
		
	}
};
main()
{
	bouncycheck bc;
	bc.check();
}	
